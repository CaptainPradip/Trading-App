<%- include('../partials/header.ejs') %>
<div class="row">
    <div class="col-1">
        <form action = '/users/login' method = 'POST' class="mt-20">
            <label for="email">Email Address</label>
            <input type="email" id="email" name="email" class="fixed-width" required>

            <label for="password">Password</label>
            <input type="password" id="password" name="password" class="fixed-width" required>
            <div class="row">
                <div class="col-1">
                    <button type="submit" class="submit_button">Login</button>
                </div>
         
            </div>
        </form>
    </div>
</div>
